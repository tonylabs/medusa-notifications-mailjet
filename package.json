{
  "name": "@gerbergpt/medusa-mailjet",
  "version": "2.0.0",
  "description": "Mailjet email delivery service for Medusa V2",
  "author": "TONYLABS <tony.wang@tonylabs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonylabs/medusa-mailjet"
  },
  "files": [
    ".medusa/server"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./workflows": {
      "types": "./.medusa/server/src/workflows/index.d.ts",
      "import": "./.medusa/server/src/workflows/index.js",
      "require": "./.medusa/server/src/workflows/index.js",
      "default": "./.medusa/server/src/workflows/index.js"
    },
    "./providers/*": {
      "types": "./.medusa/server/src/providers/*/index.d.ts",
      "import": "./.medusa/server/src/providers/*/index.js",
      "require": "./.medusa/server/src/providers/*/index.js",
      "default": "./.medusa/server/src/providers/*/index.js"
    },
    "./*": {
      "types": "./.medusa/server/src/*.d.ts",
      "import": "./.medusa/server/src/*.js",
      "require": "./.medusa/server/src/*.js",
      "default": "./.medusa/server/src/*.js"
    }
  },
  "keywords": [
    "medusa",
    "plugin",
    "medusa-plugin",
    "mailjet",
    "email",
    "notification"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "medusa plugin:develop",
    "prepublishOnly": "medusa plugin:build"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "2.11.3",
    "@medusajs/cli": "2.11.3",
    "@medusajs/framework": "2.11.3",
    "@medusajs/icons": "2.11.3",
    "@medusajs/medusa": "2.11.3",
    "@medusajs/test-utils": "2.11.3",
    "@medusajs/ui": "4.0.25",
    "@swc/core": "1.5.7",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "2.11.3",
    "@medusajs/cli": "2.11.3",
    "@medusajs/framework": "2.11.3",
    "@medusajs/icons": "2.11.3",
    "@medusajs/medusa": "2.11.3",
    "@medusajs/test-utils": "2.11.3",
    "@medusajs/ui": "^4.0.25"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@react-email/components": "^1.0.1",
    "node-mailjet": "^6.0.11"
  }
}
